@article{Song2020,
   abstract = {Polydimethylsiloxane (PDMS) is a prominent material for radiative cooling due to its promising optical properties in the mid-infrared spectral region as well as its fabrication easiness. Even though several works have reported that the mid-infrared emissivity of a PDMS film can be increased by surface modification, there is still room for further enhancement through global optimization. Here, we designed and fabricated the thin PDMS film patterned with two-dimensional gratings to obtain the highest emissivity in the wavelength range from 8 to 13 microns. A surrogate-model-based optimization was performed, and the optimum structure exhibited the averaged emissivity value of 0.99 in the wavelength of 8–13 microns, which is the highest value reported to date among polymer-based radiative coolers. For real-world applications, we also developed the fabrication method that is repeatable and applicable for various surfaces using a flexible master mold.},
   author = {Jaeman Song and Junyong Seo and Jihye Han and Jungchul Lee and Bong Jae Lee},
   issue = {9},
   journal = {Applied Physics Letters},
   month = {8},
   title = {Ultrahigh emissivity of grating-patterned PDMS film from 8 to 13 microns wavelength regime},
   volume = {117},
   year = {2020}
}
@inbook{Basu2019,
   abstract = {The importance of mass flow measurement for mass balance and batch process recipe formulation cannot be overestimated. Coriolis and thermal mass flow meters for mass flow measurement in liquid and gas are most significant. For each type and category of meter, pros and cons in various applications are covered to facilitate meter selection from the various meters available in the market. Real-life issues, including various operating conditions, environmental conditions, and available performances of meters are covered. In order to facilitate system design, specification sheets for the meters have been included. From basic Coriolis principles, comprehensive details, which include but are not limited to the theoretical and mathematical backgrounds for metering systems, etc. are described. The discussions also include specific requirements for Coriolis meter installation, pressure loss sizing, and calibration and zero stability. In addition to Coriolis, other mechanical mass flow measurements, e.g., twin-turbine mass flow meters are described. In thermal mass flow, both bypass capillary design and thermal dispersion meters are covered, with special reference to industrial use and semiconductor use. For both types of meters the theoretical background and mathematical formulation process are detailed. Installation details, selection, and calibration processes for these meters are also covered with detailed coverage of their advantages and disadvantages.},
   author = {Swapan Basu},
   city = {Amsterdam},
   doi = {10.1016/B978-0-12-812437-6.00006-8},
   booktitle = {Plant Flow Measurement and Control Handbook},
   pages = {541-613},
   publisher = {Elsevier},
   title = {Mass Flow Meter},
   url = {https://linkinghub.elsevier.com/retrieve/pii/B9780128124376000068},
   year = {2019}
}
