@article{Shockley1961,
   abstract = {In order to find an upper theoretical limit for the efficiency of p-n junction solar energy converters, a limiting efficiency, called the detailed balance limit of efficiency, has been calculated for an ideal case in which the only recombination mechanism of hole-electron pairs is radiative as required by the principle of detailed balance. The efficiency is also calculated for the case in which radiative recombination is only a fixed fraction fc of the total recombination, the rest being nonradiative. Efficiencies at the matched loads have been calculated with band gap and fc as parameters, the sun and cell being assumed to be blackbodies with temperatures of 6000°K and 300°K, respectively. The maximum efficiency is found to be 30% for an energy gap of 1.1 ev and fc = 1. Actual junctions do not obey the predicted current-voltage relationship, and reasons for the difference and its relevance to efficiency are discussed.},
   author = {William Shockley and Hans J Queisser},
   issue = {3},
   journal = {Journal of Applied Physics},
   month = {3},
   pages = {510-519},
   title = {Detailed Balance Limit of Efficiency of p-n Junction Solar Cells},
   volume = {32},
   year = {1961}
}
@article{Kumar2024,
   abstract = {In this article, lensless Fourier transform digital holographic interferometry (LLFTDHI) is presented for the investigation of heat transfer parameters such as natural convective heat transfer coefficient and local heat flux around the surface of heated wire, visual and quantitative demonstration of heat flow performances of heat sinks, temperature distribution in laminar free convective flow of liquids, and temperature measurement of human hand skin. Investigations about the temperature distribution inside the macro and micro diffusion flames as well as the study of magnetic promotion of combustion in micro diffusion flame is also presented. Advantages of using LLFT configuration of DH are as follows: 1. Image reconstruction in this geometry of DH is faster in comparison with other reconstruction methods such as Fresnel approximation and convolution approach, because in LLFTDH, Fourier transform of only recorded digital hologram is evaluated apart from some multiplicative constant, while other reconstruction methods require several FT and complex multiplication; 2. This geometry provide improved lateral resolution in the reconstructed image because entire spatial bandwidth can be utilized for complete evaluation of information from sensor by optimizing the ratio of object size and distance between object and sensor. There are some limitations of using LLFTDH. Image planes at different axial depths cannot reconstructed (i.e. digital focusing is not possible) in LLFTDH. So LLFTDH technique is not useful for applications particularly where concentrations of particles and its distribution is to be determined in 3D volume. Other limitation of LLFTDH is that it is off-axis holography so reconstruction plane has zero order term and conjugate image while in phase shifting DH provides better image quality in reconstruction because of the absence of zero-order and conjugate image. Better image reconstruction in phase shifting DH is achieved at the cost of high technical efforts: three or four phase shifted interferograms are required restricting the technique to study slowly varying phenomena.},
   author = {Varun Kumar and Chandra Shakher},
   doi = {10.1016/j.optlastec.2023.109918},
   issn = {00303992},
   journal = {Optics and Laser Technology},
   keywords = {Combustion,Convective heat transfer coefficient,Digital holographic interferometry,Heat flux,Heat transfer,Temperature},
   month = {1},
   publisher = {Elsevier Ltd},
   title = {Qualitative and quantitative investigations of temperature distribution, heat flow, and heat transfer parameters from the heated objects using lensless Fourier transform digital holography: A review},
   volume = {168},
   year = {2024}
