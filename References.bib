@article{Atkins2016,
   abstract = {Particle Image Velocimetry (PIV) is an optical measurement technique where the velocity field of an entire region within the flow is measured simultaneously. This is a fundamental difference from point measurement methods that employ probes to measure the flow velocity at a single point. A main advantage of PIV is that it is a quantitative flow field mapping technique that can provide physical insight into the overall flow behavior in question. PIV facilitates both the extraction of measurement data and the visualization of flow structures. An application-oriented introduction of planar PIV is provided with two case studies included to familiarize users with the PIV system.},
   author = {M. D. Atkins},
   doi = {10.1016/B978-0-12-809731-1.00005-8},
   isbn = {9780128098745},
   journal = {Application of Thermo-Fluidic Measurement Techniques: An Introduction},
   keywords = {Illumination,Laser,PIV,Particle tracer,Velocity field},
   month = {7},
   pages = {125-166},
   publisher = {Elsevier},
   title = {Velocity Field Measurement Using Particle Image Velocimetry (PIV)},
   year = {2016}
}
